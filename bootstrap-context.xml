<?xml version='1.0' encoding='UTF-8'?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
          http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

    <!-- The bootstrap-context.xml file is used for patch definitions, importers, 
		 workflow, and loading custom content models.  -->
	<bean id="unserializerValidatorBootstrap" abstract="true"/>

    <!-- Registration of new models -->
    <bean id="all-in-one-repo-amp.dictionaryBootstrap" parent="dictionaryModelBootstrap" depends-on="dictionaryBootstrap">
        <property name="models">
            <list>
                <value>alfresco/module/all-in-one-repo-amp/model/content-model.xml</value>
                <value>alfresco/module/all-in-one-repo-amp/model/workflow-model.xml</value>
                <value>alfresco/module/all-in-one-repo-amp/model/sgdp-carpetas.xml</value>
                <value>alfresco/module/all-in-one-repo-amp/model/sgdp-documentos.xml</value>
            </list>
        </property>
    </bean>

</beans>
